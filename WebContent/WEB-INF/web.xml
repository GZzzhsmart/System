<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>Studentservlet</servlet-name>
        <servlet-class>SQLservlet.Studentservlet</servlet-class>
    </servlet>
     <servlet>
        <servlet-name>QxServlet</servlet-name>
        <servlet-class>SQLservlet.QxServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminServlet</servlet-name>
        <servlet-class>SQLservlet.AdminServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TeacherServlet</servlet-name>
        <servlet-class>SQLservlet.TeacherServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CourseServlet</servlet-name>
        <servlet-class>SQLservlet.CourseServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClassServlet</servlet-name>
        <servlet-class>SQLservlet.ClassServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserServlet</servlet-name>
        <servlet-class>SQLservlet.UserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ScoreServlet</servlet-name>
        <servlet-class>SQLservlet.ScoreServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LeaveServlet</servlet-name>
        <servlet-class>SQLservlet.LeaveServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OutServlet</servlet-name>
        <servlet-class>SQLservlet.OutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CardServlet</servlet-name>
        <servlet-class>SQLservlet.CardServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>QxServlet</servlet-name>
        <url-pattern>/qxservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Studentservlet</servlet-name>
        <url-pattern>/studentservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminServlet</servlet-name>
        <url-pattern>/adminservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TeacherServlet</servlet-name>
        <url-pattern>/teacherservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CourseServlet</servlet-name>
        <url-pattern>/courseservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClassServlet</servlet-name>
        <url-pattern>/classservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserServlet</servlet-name>
        <url-pattern>/userservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ScoreServlet</servlet-name>
        <url-pattern>/scoreservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LeaveServlet</servlet-name>
        <url-pattern>/leaveservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OutServlet</servlet-name>
        <url-pattern>/outservlet</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
        <servlet-name>CardServlet</servlet-name>
        <url-pattern>/cardservlet</url-pattern>
    </servlet-mapping>
    
    <welcome-file-list>
		<welcome-file>/login.jsp</welcome-file>
	</welcome-file-list>
    
</web-app>